{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "storyLife": {
      "type": "richtext"
    },
    "birthday": {
      "type": "date"
    },
    "deliveryDirection": {
      "type": "component",
      "repeatable": true,
      "component": "custom.direction"
    },
    "favoriteProducts": {
      "collection": "product"
    },
    "store": {
      "via": "encargado",
      "model": "store"
    },
    "pedidos": {
      "via": "cliente",
      "collection": "pedido"
    },
    "products": {
      "via": "productores",
      "model": "product"
    },
    "RFC": {
      "type": "string"
    },
    "company": {
      "type": "string"
    }
  }
}
